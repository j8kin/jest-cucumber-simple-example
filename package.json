{
  "name": "jest-cucumber-simple-example",
  "version": "1.0.0",
  "description": "Simple example of test with jest and cucumber",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "cucumber": "cucumber-js ./feature -r ./feature/steps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j8kin/jest-cucumber-simple-example.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/j8kin/jest-cucumber-simple-example/issues"
  },
  "homepage": "https://github.com/j8kin/jest-cucumber-simple-example#readme",
  "dependencies": {
    "cucumber": "^5.1.0",
    "ts-jest": "^23.10.5"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "jest-cucumber": "^2.0.5",
    "typescript": "^3.3.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.steps.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
